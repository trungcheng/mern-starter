{
    "name": "mern-tutorial",
    "version": "1.0.0",
    "description": "MERN Stack With Redux (MySQL + Knex)",
    "main": "server.js",
    "scripts": {
        "migrate": "knex migrate:latest && knex seed:run",
        "global": "npm install -g pm2 cross-env webpack",
        "build": "NODE_ENV=production webpack --config webpack.config.js",
        "start": "npm run build && NODE_ENV=production PORT=3001 pm2 start ./server.js",
        "open": "nodemon ./server.js node_modules/ --exec babel-node --ignore client",
        "dev": "concurrently --kill-others \"npm run open\"",
        "lint": "eslint ./ --fix",
        "test": "./node_modules/.bin/mocha --recursive ./tests --compilers js:babel-register",
        "test-example": "for i in $(ls tests/); do mocha --recursive \"./tests/${i}\" | faucet ; done"
    },
    "keywords": [],
    "author": "trungcheng <trungdn.dev@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/trungcheng/mern-tutorial.git"
    },
    "license": "MIT",
